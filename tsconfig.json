{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
     "sourceMap": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "*": ["src/*"],
      "Model/*": ["src/modules/Model/*"],
      "Model": ["src/modules/Model"],
      "Presenter/*": ["src/modules/Presenter/*"],
      "Presenter": ["src/modules/Presenter"],
      "View/*": ["src/modules/View/*"],
      "View": ["src/modules/View"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
  "typeRoots": ["node_modules/@types"]
}
